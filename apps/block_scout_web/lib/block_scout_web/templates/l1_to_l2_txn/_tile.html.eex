<div
    class="fade-in txn-list-table-item"
    data-identifier-hash="<%= @l1_to_l2.queue_index %>"
>
  
    <span style="width: 10%;">
       <%= @l1_to_l2.block %>
    </span>
    <span style="width: 10%;">
      <%= @l1_to_l2.queue_index %>
    </span>
    <a style="width: 20%;" href="/tx/<%= @l1_to_l2.l2_hash %>" >
         <%= sub_hash_string(@l1_to_l2.l2_hash) %>...
       </a>
     <span style="width: 10%;" in-tile data-from-now="<%= block_timestamp(@l1_to_l2) %>">
    </span>
     <a style="width: 20%;" href="https://goerli.etherscan.io/tx/<%= @l1_to_l2.hash %>" target="_blank">
         <%= sub_hash_string(@l1_to_l2.hash) %>...
       </a>
    <a style="width: 20%;" href="https://goerli.etherscan.io/address/<%= @l1_to_l2.tx_origin %>" target="_blank">
         <%= sub_hash_string(@l1_to_l2.tx_origin) %>...
       </a>
     <span style="width: 10%;">
       <%= @l1_to_l2.gas_limit %>
    </span>
    

</div>
