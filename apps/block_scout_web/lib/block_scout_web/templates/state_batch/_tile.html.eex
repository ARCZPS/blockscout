<div 
    class="fade-in" 
    data-identifier-hash="<%= @state_batch.batch_index %>"
    style="width:100%;height:42px;display:flex;align-items:center;font-size:16px;padding:0 10px;border-bottom:1px solid #e7eaf3;"
>
  
    <span style="width: 18%;">
      <%= render "_link.html", batch_index: @state_batch.batch_index, data_test: "address_hash_link" %>
    </span>
    <span style="width: 18%;" in-tile data-from-now="<%= block_timestamp(@state_batch) %>"></span>
    <span style="width: 18%;">
      <%= @state_batch.size %>
    </span>
    <a style="width: 30%;" href="https://goerli.etherscan.io/tx/<%= @state_batch.hash %>" target="_blank">
      <%= sub_hash_string(@state_batch.hash) %>...
    </a>
    <span style="width: 16%;">
      <%= @state_batch.pre_total_elements %>
    </span>
    

</div>
